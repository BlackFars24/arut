<script>
$( document ).ready(function() {
    
    $('.uc-bf-mode2  .t-popup__container').empty();
    
    $('.uc-bf-mode2  .t-popup__container').addClass("sh-mkv").parent(".t-popup").addClass("pad-zro");    
        
        
    
    $('.uc-common-block2').appendTo('.uc-bf-mode2  .t-popup__container');
    
    $('a[href^="#popupzero"]').click(function(e) {e.preventDefault();
        setTimeout(function(){
            $('.uc-common-block2 .t396').trigger('displayChanged');
            t_lazyload_update();
        }, 10);
    });
    
    $(document).on('click','a[href="#close"], .uc-common-block2 .t396__filter', function(e){e.preventDefault();
     //$('.uc-bf-mode2  .t-popup__close').click();
    });
    
    
